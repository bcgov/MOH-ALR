<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Provides access to Complaints Management features and objects.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Complaints Management Access</label>
    <license>ComplaintsManagementPsl</license>
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>PublicComplaint</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ComplaintsManagementAccess</name>
    </userPermissions>
</PermissionSet>
