<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MustSpecifyIfInspectionTypeIsOther</fullName>
    <active>true</active>
    <description>Used by PHOCS: Inspection Type Other- Specify field id required when Inspection Type is Other</description>
    <errorConditionFormula>VisitType.Name == &apos;Other&apos; &amp;&amp;  ISBLANK(InspectionTypeOtherSpecify__c)</errorConditionFormula>
    <errorDisplayField>InspectionTypeOtherSpecify__c</errorDisplayField>
    <errorMessage>Please specify Inspection Type Other</errorMessage>
</ValidationRule>
