<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OtherConstructionMaterialRequired</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISPICKVAL(ConstructionMaterial__c, &quot;Other&quot;),
  ISBLANK(OtherConstructionMaterial__c)
)</errorConditionFormula>
    <errorDisplayField>OtherConstructionMaterial__c</errorDisplayField>
    <errorMessage>Other Construction Material is Required</errorMessage>
</ValidationRule>
